{
    "parser": "php",
    "tabWidth":2,
    "printWidth": 120,
    "singleQuote": false,
    "trailingComma": "all",
    "semi":true,
    "plugins": ["prettier-plugin-blade", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": [
        "*.blade.php"
      ],
      "options": {
        "parser": "blade"
      }
    }
  ]
}
